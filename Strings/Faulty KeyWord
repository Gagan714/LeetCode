class Solution {
    public String finalString(String s) {
        StringBuilder sc=new StringBuilder();
        char[]s1=s.toCharArray();
        for(int i=0;i<s1.length;i++){
            if(s1[i]!='i'){
              sc=sc.append(s1[i]);
            }else if(s1[i]=='i'){
               sc= sc.reverse();
            }

        }
        return sc.toString();
    }
}
