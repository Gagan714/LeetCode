class Solution {
    public int[] shuffle(int[] nums, int n) {
        int count=0;
        int []output=new int[nums.length];
        int i=0;
        while(i<nums.length&&n<nums.length){
            if(count%2==0){
                output[count]=nums[i++];
            }else{
                output[count]=nums[n++];
            }
            count++;
        }
        return output;
    }
}
